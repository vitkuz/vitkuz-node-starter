extends ../../../views/rootLayout

include ../../../views/mixins/shelfs/printStoriesShelf

block globalNav
    include ../../../views/partials/globalNav

block content
    - stories = content.stories;

    .div.article
        .article_header.pt-3.pb-3
            p= createdAt
            h1= title
            h2= subtitle
            p= description
            each menuItem in menu
                a(href='#'+menuItem)= menuItem
        .article_content
            +printStoriesShelf(stories)






